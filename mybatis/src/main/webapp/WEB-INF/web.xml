<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>CustomerServlet</servlet-name>
        <servlet-class>com.ai.servlet.CustomerServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>DeleteOneServlet</servlet-name>
        <servlet-class>com.ai.servlet.DeleteOneServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteBatchServlet</servlet-name>
        <servlet-class>com.ai.servlet.DeleteBatchServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>InitTalkServlet</servlet-name>
        <servlet-class>com.ai.servlet.InitTalkServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CustomerServlet</servlet-name>
        <url-pattern>/customer</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DeleteOneServlet</servlet-name>
        <url-pattern>/DeleteOne</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DeleteBatchServlet</servlet-name>
        <url-pattern>/DeleteBatch</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>InitTalkServlet</servlet-name>
        <url-pattern>/InitTalk</url-pattern>
    </servlet-mapping>
</web-app>
